    "helmet": "3.21.3",
    "regenerator-runtime": "0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "nodemon": "2.0.2",
    "semistandard": "14.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
